{
    "name": "ylsideas/cockroachdb-laravel",
    "description": "This is my package cockroachdb-laravel",
    "keywords": [
        "ylsideas",
        "laravel",
        "cockroachdb-laravel",
        "cockroachdb"
    ],
    "homepage": "https://github.com/ylsideas/cockroachdb-laravel",
    "license": "MIT",
    "authors": [
        {
            "name": "Peter Fox",
            "email": "peter.fox@ylsideas.co",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/contracts": "12.*|11.*|10.*|9.*",
        "spatie/laravel-package-tools": "^1.9.2"
    },
    "require-dev": {
        "doctrine/dbal": "^3.2",
        "laravel/pint": "^1.2",
        "nunomaduro/collision": "^8.1|^7.2|^6.0|^5.10",
        "orchestra/testbench": "^10.0|^9.0|^8.0|^7.0",
        "pestphp/pest": "^3.0|^2.23|^1.21",
        "pestphp/pest-plugin-laravel": "^3.0|^2.2|^1.1"
    },
    "autoload": {
        "psr-4": {
            "YlsIdeas\\CockroachDb\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "YlsIdeas\\CockroachDb\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest coverage",
        "lint": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "YlsIdeas\\CockroachDb\\CockroachDbServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
